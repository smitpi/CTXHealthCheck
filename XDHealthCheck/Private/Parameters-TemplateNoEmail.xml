<?xml version="1.0" encoding="utf-8"?>
<settings>
	<Variables>
		<Variable>
			<Name>CTXDDC</Name>
			<Value></Value>
			<!-- A Single Citrix Data Collector FQDN  -->
			<Type>[string]</Type>
			<Scope>Local</Scope>
		</Variable>
		<Variable>
			<Name>CTXStoreFront</Name>
			<Value></Value>
			<!-- A Single Citrix StoreFront FQDN -->
			<Type>[string]</Type>
			<Scope>Local</Scope>
		</Variable>
		<Variable>
			<Name>RDSLicensServer</Name>
			<Value></Value>
			<!-- RDS LicenseServer FQDN -->
			<Type>[string]</Type>
			<Scope>Local</Scope>
		</Variable>
		<Variable>
			<Name>RDSLicensType</Name>
			<Value></Value>
			<!-- Add RDS License Type (Per Device / Per User) -->
			<Type>[string]</Type>
			<Scope>Local</Scope>
		</Variable>
		<Variable>
			<Name>ReportsFolder</Name>
			<Value></Value>
			<!-- Path to the Reports Folder -->
			<Type>[string]</Type>
			<Scope>Local</Scope>
		</Variable>
		<Variable>
			<Name>ParametersFolder</Name>
			<Value></Value>
			<!-- Path to where the Parameters.xml will be saved -->
			<Type>[string]</Type>
			<Scope>Local</Scope>
		</Variable>
		<Variable>
			<Name>DashboardTitle</Name>
			<Value></Value>
			<!-- Title to be used in the reports and Dashboard -->
			<Type>[string]</Type>
			<Scope>Local</Scope>
		</Variable>
		<Variable>
			<Name>SaveExcelReport</Name>
			<Value></Value>
			<!-- Save Config Changes and eventlogs to an excel report (true / false) -->1
			<Type>[bool]</Type>
			<Scope>Local</Scope>
		</Variable>
	</Variables>
</settings>
