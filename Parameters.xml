<?xml version="1.0" encoding="utf-8"?>
<settings>
  <Variables>
    <Variable>
      <Name>CTXDDC</Name>
      <Value>ZAPRNBCTX0002.corp.dsarena.com</Value>
      <!-- A Single Citrix Data Collector FQDN  -->
      <Type>[string]</Type>
      <Scope>Local</Scope>
    </Variable>
    <Variable>
      <Name>CTXStoreFront</Name>
      <Value>ZAPRNBCTX0004.corp.dsarena.com</Value>
      <!-- A Single Citrix StoreFront FQDN -->
      <Type>[string]</Type>
      <Scope>Local</Scope>
    </Variable>
    <Variable>
      <Name>RDSLicensServer</Name>
      <Value>ZAPSDCCTX0007.corp.dsarena.com</Value>
      <!-- RDS LicenseServer FQDN -->
      <Type>[string]</Type>
      <Scope>Local</Scope>
    </Variable>
    <Variable>
      <Name>RDSLicensType</Name>
      <Value>Per Device</Value>
      <!-- Add RDS License Type (Per Device / Per User) -->
      <Type>[string]</Type>
      <Scope>Local</Scope>
    </Variable>
    <Variable>
      <Name>SaveExcelReport</Name>
      <Value>true</Value>
      <!-- Save Config Changes and eventlogs to an excel report (true / false) -->
      <Type>[bool]</Type>
      <Scope>Local</Scope>
    </Variable>
    <!-- E-Mail Configuration -->
    <Variable>
      <Name>SendEmail</Name>
      <Value>false</Value>
      <!-- Send Report with email (true / false)  -->
      <Type>[bool]</Type>
      <Scope>Local</Scope>
    </Variable>
    <Variable>
      <Name>emailFrom</Name>
      <Value>WorkSpaceHealth@absa.co.za</Value>
      <!-- Address of the sender -->
      <Type>[string]</Type>
      <Scope>Local</Scope>
    </Variable>
    <Variable>
      <Name>emailTo</Name>
      <Value>pierre.smit2@absa.africa</Value>
      <!-- Address of the recipient -->
      <Type>[string]</Type>
      <Scope>Local</Scope>
    </Variable>
    <Variable>
      <Name>smtpServer</Name>
      <Value>mail.absa.co.za</Value>
      <!-- IP or name of SMTP server  -->
      <Type>[string]</Type>
      <Scope>Local</Scope>
    </Variable>
    <Variable>
      <Name>smtpServerPort</Name>
      <Value>25</Value>
      <!-- Port of SMTP server -->
      <Type>[string]</Type>
      <Scope>Local</Scope>
    </Variable>
    <Variable>
      <Name>smtpEnableSSL</Name>
      <Value>true</Value>
      <!-- Use ssl for SMTP or not(False or True) -->
      <Type>[bool]</Type>
      <Scope>Local</Scope>
    </Variable>
  </Variables>
</settings>